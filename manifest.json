{
  "manifest_version": 3,
  "name": "Startup Metrics Dashboard",
  "description": "View your startup metrics from private Google Sheets on every new tab",
  "version": "1.0",
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "permissions": [
    "storage",
    "identity"
  ],
  "host_permissions": [
    "https://sheets.googleapis.com/*"
  ],
  "oauth2": {
    "client_id": "304726963595-ur5bvskdm1qqv1vsth8g1han9bafvl6i.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/spreadsheets.readonly"
    ]
  }
} 